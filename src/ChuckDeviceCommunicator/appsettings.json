{
  "AllowedHosts": "*",
  "GrpcConfiguratorServer": "http://localhost:5002",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "Endpoints": {
      "Grpc": {
        "Url": "http://*:5003",
        "Protocols": "Http2"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Relay": {
    "MaximumRetryCount": 3,
    "RequestTimeout": 15
  }
}
