@using ChuckDeviceController.Common
@model ProtoDataStatisticsViewModel
@{
    ViewData["Title"] = "Data Entity Benchmarks";
}

<h1>Data Entity Benchmarks</h1>
<br />

<p>
    <a class="btn btn-danger btn-sm float-end" asp-action="Reset">Reset</a>
</p>
<br />

<div class="card text-center">
    <div class="card-header text-center"><b>Overview</b></div>
    <div class="card-body">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-md-6 p-3">
                    <div class="list-group">
                        <div class="list-group-item">
                            <h3 class="pull-right"><i class="fa-solid fa-gauge-simple"></i></h3>
                            <h4 class="list-group-item-heading">Average Count</h4>
                            <p class="list-group-item-text">@((Model?.AverageTime?.Count ?? 0).ToString("N0")) entities</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 p-3">
                    <div class="list-group">
                        <div class="list-group-item">
                            <h3 class="pull-right"><i class="fa-solid fa-clock"></i></h3>
                            <h4 class="list-group-item-heading">Average Time(sec)</h4>
                            <p class="list-group-item-text">@(Model?.AverageTime?.TimeS ?? 0)s</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row justify-content-center">
                <div class="col-md-6 p-3">
                    <div class="list-group">
                        <div class="list-group-item">
                            <h3 class="pull-right"><i class="fa-solid fa-gauge-simple"></i></h3>
                            <h4 class="list-group-item-heading">@Html.DisplayNameFor(model => model.TotalProtoPayloadsReceived)</h4>
                            <p class="list-group-item-text">@Html.DisplayFor(model => model.TotalProtoPayloadsReceived)</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 p-3">
                    <div class="list-group">
                        <div class="list-group-item">
                            <h3 class="pull-right"><i class="fa-solid fa-clock"></i></h3>
                            <h4 class="list-group-item-heading">@Html.DisplayNameFor(model => model.TotalProtosProcessed)</h4>
                            <p class="list-group-item-text">@Html.DisplayFor(model => model.TotalProtosProcessed)</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row justify-content-center">
                <div class="col-md-6 p-3">
                    <div class="list-group">
                        <div class="list-group-item">
                            <h3 class="pull-right"><i class="fa-solid fa-gauge-simple"></i></h3>
                            <h4 class="list-group-item-heading">@Html.DisplayNameFor(model => model.TotalEntitiesProcessed)</h4>
                            <p class="list-group-item-text">@Html.DisplayFor(model => model.TotalEntitiesProcessed)</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 p-3">
                    <div class="list-group">
                        <div class="list-group-item">
                            <h3 class="pull-right"><i class="fa-solid fa-clock"></i></h3>
                            <h4 class="list-group-item-heading">@Html.DisplayNameFor(model => model.TotalEntitiesUpserted)</h4>
                            <p class="list-group-item-text">@Html.DisplayFor(model => model.TotalEntitiesUpserted)</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<br>

