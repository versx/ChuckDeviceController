    <div class="col">
        <dt class="col-sm-10">@Html.DisplayNameFor(model => model.TotalDatabaseConnectionsCreated)</dt>
        <dd class="col-sm-2">@Html.DisplayFor(model => model.TotalDatabaseConnectionsCreated)</dd>
