{
  "AllowedHosts": "*",
  "Authentication": {
    "Discord": {
      "Enabled": false,
      "ClientId": "",
      "ClientSecret": ""
    },
    "GitHub": {
      "Enabled": false,
      "ClientId": "",
      "ClientSecret": ""
    },
    "Google": {
      "Enabled": false,
      "ClientId": "",
      "ClientSecret": ""
    }
  },
  "AutomaticMigrations": true,
  "Cache": {
    "SizeLimit": 10240,
    "ExpirationScanFrequency": 15,
    "CompactionPercentage": 0.25
  },
  "ConnectionStrings": {
    "DefaultConnection": "Uid=cdcuser;Password=cdcpass123;Host=127.0.0.1;Port=3306;Database=cdcdb;old guids=true;Allow User Variables=true;"
  },
  "Jwt": {
    "Enabled": false,
    "Issuer": "http://127.0.0.1:8881/",
    "Audience": "http://127.0.0.1:8881/",
    "Key": "JwtExampleSecretKey_MakeSureYouChangeThisToA_SecureRandomizedValue",
    "TokenValidityM": 30
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "Endpoints": {
      "Http": {
        "Url": "http://*:8881"
      },
      "Grpc": {
        "Url": "http://*:5002",
        "Protocols": "Http2"
      }
    }
  },
  "Keys": {
    "SendGridKey": ""
  },
  "Locale": "en",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "LogLevel": {
        "ChuckDeviceConfigurator": "Information",
        "ChuckDeviceController": "Information",
        "Microsoft.EntityFrameworkCore.Database": "Warning",
        "Microsoft.EntityFrameworkCore.Model.Validation": "Warning",
        "Microsoft.EntityFrameworkCore.Update": "Warning",
        "Microsoft.AspNetCore.Diagnostics.DeveloperExceptionPageMiddleware": "None",
        "Default": "Warning"
      }
    }
  },
  "Map": {
    "StartLatitude": 0,
    "StartLongitude": 0,
    "StartZoom": 13,
    "MinimumZoom": 4,
    "MaximumZoom": 18,
    "TileserverUrl": "https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png"
  },
  "Theme": "dark"
}