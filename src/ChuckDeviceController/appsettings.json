{
  "AllowedHosts": "*",
  "AutomaticMigrations": true,
  "Cache": {
    "CompactionPercentage": 0.25,
    "EntityExpiryLimitM": 30,
    "ExpirationScanFrequencyM": 5,
    "SizeLimit": 10240
  },
  "ConnectionStrings": {
    "DefaultConnection": "Uid=cdcuser;Password=cdcpass123;Host=127.0.0.1;Port=3306;Database=cdcdb;old guids=true;Allow User Variables=true;"
  },
  "ConvertMadData": false,
  "DbContextPoolSize": 1024,
  "Grpc": {
    "Configurator": "http://localhost:5002",
    "Communicator": "http://localhost:5003"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Grpc.Core": "None",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Diagnostics.DeveloperExceptionPageMiddleware": "None",
      "Microsoft.EntityFrameworkCore": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Diagnostics.DeveloperExceptionPageMiddleware": "None",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    }
  },
  "ProcessingOptions": {
    "Protos": {
      "LogLevel": "Summary",
      "ProcessMapPokemon": true,
      "Queue": {
        "MaximumBatchSize": 100,
        "MaximumSizeWarning": 500,
        "MaximumQueueCapacity": 8192
      }
    },
    "Data": {
      "ClearOldForts": true,
      "IntervalS": 3,
      "LogLevel": "Summary",
      "ProcessPlayerData": false,
      "ProcessCells": true,
      "ProcessWeather": true,
      "ProcessForts": true,
      "ProcessFortDetails": true,
      "ProcessGymInfo": true,
      "ProcessGymDefenders": true,
      "ProcessGymTrainers": true,
      "ProcessWildPokemon": true,
      "ProcessNearbyPokemon": true,
      "ProcessMapPokemon": true,
      "ProcessQuests": true,
      "ProcessEncounters": true,
      "ProcessDiskEncounters": true,
      "Queue": {
        "MaximumBatchSize": 500,
        "MaximumSizeWarning": 1024,
        "MaximumQueueCapacity": 8192
      }
    },
    "Consumer": {
      "IntervalS": 5,
      "LogLevel": "Summary",
      "MaximumBatchSize": 1000,
      "QueueCapacity": 1048576,
      "QueueConcurrencyLevelMultiplier": 2
    }
  },
  "StatisticTriggers": {
    "Pokemon": false
  },
  "Urls": "http://*:8888"
}
